[run]
source = .
omit = 
    */migrations/*
    */tests.py
    */test_*.py
    */__pycache__/*
    */venv/*
    */env/*
    manage.py
    */settings*.py
    */wsgi.py
    */asgi.py
    */conftest.py
    */factories/*

[report]
precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov

